.profile
	.box
		height 50%;
		width 50%;
		margin 25px
		background-color blue
		position absolute
	.span3, .span4
		.community
			.image
				height 250px
				border-radius 6px
				border  2px solid #FAFAFA
				box-shadow  0 1px 2px rgb(34, 25, 25)
				box-shadow  0 1px 2px rgba(34, 25, 25, 0.4)
				position relative
				text-align center
				.footer
					background-color rgba(100,236,255,0.85)
					padding 10px
					font-family wisdom_scriptregular
					color white
					border-radius 4px 4px 0 0 
					.title
						font-size 30px
					.desc
						font-size 15px
						font-family 'Lato'
				button
					position relative
					top 150px
	.lead
		line-height 30px
		font-size 35px